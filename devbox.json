{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.10.7/.schema/devbox.schema.json",
  "packages": [
    "go@1.24.1"
  ],
  "env": {
    "NIX_HARDENING_ENABLE": ""
  },
  "shell": {
    "scripts": {
      "build": "go build ./bin/main.go",
      "render": "mkdir -p output && go run ./bin/main.go render output/output.scad"
    }
  }
}
